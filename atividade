cartao = input().strip()


digitos = [int(d) for d in cartao]


impares = sum(digitos[-1::-2])


pares = []
for d in digitos[-2::-2]:
    dobro = d * 2
    if dobro < 10:
        pares.append(dobro)
    else:
        pares.append(dobro - 9)  # equivalente a "dobro - 10 + 1"

total = impares + sum(pares)


if total % 10 == 0:
    print("Cartão válido")
else:
    print("Cartão inválido")
